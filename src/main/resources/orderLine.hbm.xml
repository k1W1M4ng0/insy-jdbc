<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  

<hibernate-mapping>  
    <class name="insy.beans.OrderLine" table="order_lines">  
        <id name="id" type="integer">  
            <generator class="identity"></generator>  
        </id>  
        <many-to-one name="article" column="article_id" class="insy.beans.Article"></many-to-one>  
        <many-to-one name="order" column="order_id" class="insy.beans.Order"></many-to-one>  
        <property name="amount" type="integer"></property>  
    </class>  
</hibernate-mapping>  

